---
import CardView from './CardView.astro';
import type { cardItem } from '../interfaces/cardInterface';

interface Props {
	cards:cardItem[];
}
const {cards}=Astro.props;
---

<div class="seccion max-w-[120rem] w-[95%] my-0 mx-auto ">
		<div class="md:flex md:justify-between md:flex-wrap ">
		{
			cards.map(card => <CardView {...card}/>)
		}
		<slot/>
	</div>
</div>